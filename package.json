{
  "name": "multilanguage-flashcard",
  "version": "1.0.4",
  "description": "Learn 27 languages with emojis on Multilanguage flashcard.",
  "homepage": "https://multilanguage-flashcard.codewithshin.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/multilanguage-flashcard"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@playwright/test": "^1.42.0",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.18",
    "emojibase-data": "^15.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.36.0-next.4",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "svelte": "5.0.0-next.69",
    "svelte-5-ui-lib": "^0.2.46",
    "svelte-check": "^3.6.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "type": "module"
}
